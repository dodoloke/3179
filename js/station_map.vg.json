{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 450,
    "title": "Estimated Annual Temperature (Celsius) in Australia, 2020-2039",
    "projection": {"type": "equalEarth"},
    "layer": [
      {
        "data": {
          "url": "https://raw.githubusercontent.com/dodoloke/3179/refs/heads/main/js/AusMapWithGraticules.topojson",
          "format": {"type": "topojson", "feature": "ne_10m_admin_0_countries"}
        },
        "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "white"}
      },
      {
        "data": {
          "url": "https://raw.githubusercontent.com/dodoloke/3179/refs/heads/main/js/AusMapWithGraticules.topojson",
          "format": {"type": "topojson", "feature": "ne_10m_graticules_10"}
        },
        "mark": {"type": "geoshape", "fill": null, "stroke": "lightgray"}
      },
      {
        "data": {
          "url": "https://raw.githubusercontent.com/dodoloke/3179/refs/heads/main/data/tas_aus-station_r1i1p1_CSIRO-MnCh-wrt-1986-2005-Scl_v1_mon_seasavg-clim_1.csv"
        },
        "transform": [
            {"filter": "datum.ENSEMBLE == 'r1i1p1' && datum.RCP == 'rcp26' && datum.MODEL == 'CESM1-CAM5' && datum.CLIMATOLOGY == '2020-2039'"},
            {"calculate": "toNumber(datum.LAT)", "as": "latitude"},
            {"calculate": "toNumber(datum.LON)", "as": "longitude"},
            {"calculate": "toNumber(datum.Annual)", "as": "annual_c"}
        ],
        "mark": {"type": "circle", "tooltip": {"content": "data"}},
        "encoding": {
          "longitude": {"field": "longitude", "type": "quantitative"},
          "latitude": {"field": "latitude", "type": "quantitative"},
          "color": {
            "field": "annual_c",
            "type": "quantitative",
            "title": "Annual Mean (Celsius)",
            "scale": {"scheme": "reds"}
          }
        }
      }
    ]
  }